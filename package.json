{
  "name": "celled",
  "version": "1.6.1",
  "description": "Simple javascript cell editor",
  "main": "dist/celled.js",
  "module": "dist/celled.es5.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clear && npm run build:bundle && npm run build:docs",
    "build:clear": "rimraf dist docs",
    "build:bundle": "rollup -c",
    "build:docs": "copyfiles -u 1 dist/* docs",
    "start": "rollup -cw",
    "test": "jest --watchAll",
    "release": "npm run build && npm publish"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/hansmaad/celled.git"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@testing-library/dom": "^7.30.4",
    "@testing-library/user-event": "^13.1.5",
    "@types/jest": "^24.0.18",
    "copyfiles": "^2.1.1",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
