{
  "name": "celled",
  "version": "2.0.5",
  "description": "Simple javascript cell editor",
  "main": "dist/celled.js",
  "module": "dist/celled.es6.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clear && npm run build:bundle && npm run build:types && npm run build:docs",
    "build:clear": "rimraf dist docs",
    "build:bundle": "rollup -c",
    "build:docs": "copyfiles -u 1 dist/* docs",
    "build:types": "tsc --project tsconfig.types.json",
    "start": "rollup -cw",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "release": "npm run build && npm publish"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/hansmaad/celled.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "copyfiles": "^2.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "sass": "^1.59.3",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "tslint": "^5.20.0",
    "typescript": "^4.9.5"
  }
}
